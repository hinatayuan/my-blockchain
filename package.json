{
  "name": "my-blockchain",
  "version": "1.0.0",
  "description": "A simple blockchain implementation with CosmJS integration and React frontend",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@cosmjs/crypto": "^0.31.0",
    "@cosmjs/encoding": "^0.31.0",
    "@cosmjs/proto-signing": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "blockchain",
    "cosmjs",
    "cryptocurrency",
    "mining",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT"
}